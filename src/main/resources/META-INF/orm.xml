<?xml version='1.0' encoding='UTF-8'?>
<entity-mappings xmlns='http://java.sun.com/xml/ns/persistence/orm'
	xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:schemaLocation='http://java.sun.com/xml/ns/persistence/orm
	http://java.sun.com/xml/ns/persistence/orm_2_0.xsd'
	version='2.0'>
	<named-query name="Product.findMinPrijs">
	    <query>
	        select min(prijs) from Product
	    </query>
	</named-query>
	<named-query name="Product.findMaxPrijs">
	    <query>
	        select max(prijs) from Product
	    </query>
	</named-query>
	<named-query name="Product.findMinJaartal">
	    <query>
	        select min(jaartal) from Product
	    </query>
	</named-query>
	<named-query name="Product.findMaxJaartal">
	    <query>
	        select max(jaartal) from Product
	    </query>
	</named-query>
</entity-mappings>